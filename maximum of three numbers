import java.util.*;

public class Solution {

    static int largest(int a, int b, int c) {
        int max = a;

        if (b > max) {
            max = b;
        }

        if (c > max) {
            max = c;
        }

        return max;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        Solution ob=new Solution();

        int a = sc.nextInt();
        int b = sc.nextInt();
        int c = sc.nextInt();
        int e=ob.largest(a,b,c);

        System.out.println(e);
    }
}
