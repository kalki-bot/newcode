import java.io.*;
import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        
        int R = scan.nextInt();
        int C = scan.nextInt();
        
        int sum = 0;
        
        for(int i = 0; i < R; i++) {
            for(int j = 0; j < C; j++) {
                int num = scan.nextInt();
                sum += num;
            }
        }
        
        System.out.println(sum);
    }
}
