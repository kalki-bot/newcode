import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        
        String S1 = sc.nextLine();
        String S2 = sc.nextLine();
        
        
        String result = S1 + S2;
        
        System.out.println(result);
        
        sc.close();
    }
}
